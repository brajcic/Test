pipeline {
    agent any
    stages {
        stage('First stage') {
            steps {
                script {
                    echo "Hello"
                }
            }
            post {
                success {
                    
                        echo "Success!"
                    
                }
            }
        }
        stage('Second stage') {
            steps {
                script {
                    println(params.CHECKBOX)
                    println(params.CHOICE)
                    println(params.PASS)
                    println(params.STRING)
                }
            }
        }
    }
}
