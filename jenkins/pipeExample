@Library('jenkins-library') _

pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo 'Building'
            }
        }
        stage('Shared library') {
            steps {
                script {
                    echo jenkins_date.getTime()
                }
            }
        }
        stage('Call another job') {
            steps {
                build job: 'simple-pipe'
            }
        }
        stage('Git cloning') {
            steps {
                script {
                    echo git_commands.gitClone()
                }
            }
        }
        stage('Git clone private') {
            steps {
                 git credentialsId: 'Github', url: 'https://github.com/brajcic/private_rep.git'
            }
        }
        stage('Maven commands') {
            steps {
                script {
                    maven_commands.installMaven()
                }
            }
        }
        stage('Done') {
            steps {
                echo 'Done'
            }
        }
    }
}