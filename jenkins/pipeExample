@Library('jenkins-library@') _

pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo 'Building'
            }
        }
        stage('Shared library') {
            steps {
                script {
                    echo jenkinsDate.getCurrentDate()
                }
            }
        }
        stage('Done') {
            steps {
                echo 'Done'
            }
        }
    }
}